<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=" http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

<bean id="planet"  class="com.dev.devapp.bean.PlanetBean">
<property name="planetId" value="3"></property>
<property name="planetName" value="Earth"></property>
<property name="revolutionSpeed" value="365"></property>
<property name="waterAvailable" value="true"></property>
</bean>



</beans>
